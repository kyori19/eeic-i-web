{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "jsx": "preserve",
    "incremental": true,
    "outDir": "../public",
    "tsBuildInfoFile": ".tsbuildinfo"
  },
  "include": ["**/*.ts"],
  "exclude": []
}
